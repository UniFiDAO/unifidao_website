<p id="notice"><%= notice %></p>

<h1>Email Links</h1>

<table>
  <thead>
    <tr>
      <th>Token</th>
      <th>Expires at</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @email_links.each do |email_link| %>
      <tr>
        <td><%= email_link.token %></td>
        <td><%= email_link.expires_at %></td>
        <td><%= email_link.user_id %></td>
        <td><%= link_to 'Show', email_link %></td>
        <td><%= link_to 'Edit', edit_email_link_path(email_link) %></td>
        <td><%= link_to 'Destroy', email_link, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Email Link', new_email_link_path %>
